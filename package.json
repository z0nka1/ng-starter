{
  "name": "ng-starter",
  "version": null,
  "scripts": {
    "ng": "ng",
    "start": "npm run lint && node --max_old_space_size=2120 ./node_modules/@angular/cli/bin/ng serve  --port 8081 -c=local",
    "start:hmr": "npm run lint && node --max_old_space_size=2120 ./node_modules/@angular/cli/bin/ng serve  --port 8081 --hmr -c=hmr",
    "start:dev": "npm run lint && node --max_old_space_size=2120 ./node_modules/@angular/cli/bin/ng serve  --port 8081 -c=dev",
    "start:test": "npm run lint && node --max_old_space_size=2120 ./node_modules/@angular/cli/bin/ng serve  --port 8081 -c=test",
    "start:uat": "npm run lint && node --max_old_space_size=2120 ./node_modules/@angular/cli/bin/ng serve  --port 8081 -c=uat",
    "start:prod": "npm run lint && node --max_old_space_size=2120 ./node_modules/@angular/cli/bin/ng serve  --port 8081 -c=prod",
    "start:proxy": "npm run lint && node --max_old_space_size=2120 ./node_modules/@angular/cli/bin/ng serve  --port 8081 --proxy-config proxy.conf.json",
    "aot:local": "npm run lint && node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer  --aot --source-map -c=local",
    "aot:dev": "npm run lint && node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer --aot --source-map -c=dev",
    "aot:test": "npm run lint && node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer --aot -c=test",
    "aot:uat": "npm run lint && node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer --aot -c=uat",
    "aot:prod": "npm run lint && node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer --aot -c=prod",
    "build": "ng build",
    "test": "npm run lint && node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng test",
    "lint": "node ./node_modules/@angular/cli/bin/ng lint",
    "pretty-quick": "pretty-quick --staged",
    "e2e": "ng e2e",
    "gitlab-ci": "node ./scripts/ci/compare-pk.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "exit": "echo '执行完毕'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.2",
    "@angular/common": "^7.2.2",
    "@angular/compiler": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "@delon/theme": "^1.4.5",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.3",
    "less": "^2.7.3",
    "moment": "^2.24.0",
    "ng-zorro-antd": "7.0.3",
    "rebirth-http": "^6.0.2",
    "rebirth-storage": "^6.0.2",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.2.2",
    "ts-md5": "^1.2.4",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.3",
    "@angular/cli": "^7.2.3",
    "@angular/compiler-cli": "^7.2.2",
    "@angular/language-service": "^7.2.2",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.19",
    "codelyzer": "^4.5.0",
    "conventional-changelog-cli": "^2.0.12",
    "husky": "^1.3.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "protractor": "^5.4.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "3.1.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./scripts/git/commit-msg.js -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run gitlab-ci && pretty-quick --staged"
    }
  }
}
